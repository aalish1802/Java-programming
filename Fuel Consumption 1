import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
        double liters = sc.nextDouble();
        double distance = sc.nextDouble();
      if (liters <= 0 || distance <= 0) 
     {
       System.out.println("Invalid Input");
       return;
     }
       double literskm = (liters / distance) * 100;
       double miles = distance * 0.6214;
       double gallons = liters * 0.2642;
       double milesgal = miles / gallons;
       System.out.printf("Liters/100KM\n%.2f\n", literskm);
       System.out.printf("Miles/gallons\n%.2f\n", milesgal);
           }
        }
