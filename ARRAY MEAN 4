import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int arr[] = new int[a];
        int sum = 0;
        
        for(int i = 0; i < a; i++) { 
            arr[i] = sc.nextInt();
            sum=sum+ arr[i];
        }
        float m= sum / a;
        System.out.printf("Array Mean Value is %.2f", m);
    }
}
